<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane style="-fx-background-color: F1F8E9;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pt.ipp.isep.dei.esoft.project.ui.gui.controller.DevTeamController">
   <top>
      <Text fill="#bb6140" strokeType="OUTSIDE" strokeWidth="0.0" text="Byte Masters" BorderPane.alignment="CENTER">
         <font>
            <Font name="Bradley Hand ITC" size="45.0" />
         </font>
         <BorderPane.margin>
            <Insets top="10.0" />
         </BorderPane.margin>
      </Text>
   </top>
   <center>
      <TableView fx:id="tvTeam" prefHeight="157.0" prefWidth="320.0" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="name" editable="false" minWidth="50.0" prefWidth="159.0" text="Name" />
          <TableColumn fx:id="email" editable="false" minWidth="50.0" prefWidth="159.0" text="Email" />
        </columns>
         <BorderPane.margin>
            <Insets bottom="20.0" left="100.0" right="100.0" top="20.0" />
         </BorderPane.margin>
      </TableView>
   </center>
   <bottom>
      <Button fx:id="goBackButton" mnemonicParsing="false" onAction="#handleGoBackButtonAction" prefHeight="25.0" prefWidth="57.0" style="-fx-background-color: #80CBC4; -fx-text-fill: white; -fx-background-radius: 15;" text="BACK" BorderPane.alignment="BOTTOM_LEFT">
         <BorderPane.margin>
            <Insets bottom="20.0" left="20.0" />
         </BorderPane.margin></Button>
   </bottom>
</BorderPane>
