<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="387.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pt.ipp.isep.dei.esoft.project.ui.gui.ui.Uss.AddAgendaEntryUI">
   <children>
      <Text fill="#ff0101" strokeType="OUTSIDE" strokeWidth="0.0" text="GSM | Add New Entry to the Agenda">
         <font>
            <Font name="System Bold" size="28.0" />
         </font>
         <VBox.margin>
            <Insets left="10.0" top="10.0" />
         </VBox.margin>
      </Text>
      <HBox prefHeight="57.0" prefWidth="850.0">
         <children>
            <Label text="Green Space: ">
               <padding>
                  <Insets right="10.0" />
               </padding>
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Label>
            <ComboBox fx:id="cbGreenSpace" prefHeight="51.0" prefWidth="331.0" promptText="Select Green Space" />
            <Label text="Task: ">
               <padding>
                  <Insets right="10.0" />
               </padding>
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Label>
            <ComboBox fx:id="cbTask" prefHeight="51.0" prefWidth="212.0" promptText="Select Task">
               <padding>
                  <Insets right="10.0" />
               </padding></ComboBox>
         </children>
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </HBox>
      <HBox prefHeight="51.0" prefWidth="536.0">
         <children>
            <Label text="Start Date: ">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
               <padding>
                  <Insets right="10.0" />
               </padding>
            </Label>
            <DatePicker fx:id="dpStartDate" prefHeight="51.0" prefWidth="248.0" />
            <ComboBox fx:id="cbStartHour" prefHeight="51.0" prefWidth="220.0" promptText="Select Hour">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </ComboBox>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" top="20.0" />
         </VBox.margin>
      </HBox>
      <HBox prefHeight="69.0" prefWidth="487.0">
         <children>
            <Label text="End Date: ">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
               <padding>
                  <Insets right="18.0" />
               </padding>
            </Label>
            <DatePicker fx:id="dpEndDate" prefHeight="25.0" prefWidth="248.0" />
            <ComboBox fx:id="cbEndHour" prefHeight="51.0" prefWidth="220.0" promptText="Select Hour">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </ComboBox>
         </children>
         <VBox.margin>
            <Insets top="5.0" />
         </VBox.margin>
      </HBox>
      <HBox prefHeight="53.0" prefWidth="572.0">
         <children>
            <Button fx:id="btnAddAgendaEntry" mnemonicParsing="false" text="Add Agenda Entry">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="btnCancel" mnemonicParsing="false" text="Cancel">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
