@startuml

class Competence {
    - id: String
    - description: String
    - level: String
}

class Employee {
    - id: String
    - name: String
    - competences: List<Competence>

    + addCompetence(competence: Competence)
    + removeCompetence(competenceId: String)
    + getCompetences(): List<Competence>
}

class Organization {
    - name: String
    - employees: List<Employee>

    + addEmployee(employee: Employee)
    + removeEmployee(employeeId: String)
    + getEmployee(employeeId: String): Employee
}

Employee "1" --> "*" Competence : possesses
Organization "1" --> "*" Employee : employs

@enduml
