@startuml
skinparam monochrome true
skinparam packageStyle rectangle
skinparam shadowing false

'left to right direction

skinparam classAttributeIconSize 0

hide circle
hide methods

class Collaborator {
   name
   birthdate
   admissionDate
   address
   contactInfo
   idDocumentType
   idDocumentNumber
}

class Skill {
    name
}

class HRM {
   name
   birthdate
   admissionDate
   address
   contactInfo
   idDocumentType
   idDocumentNumber
}
class GreenSpace {
    name
    address
    type
}

class Garden {
    name
    address
    dimension
}

class MediumSizedPark {
    name
    address
    dimension
}

class LargeSizedPark {
    name
    address
    dimension
}

GreenSpace <|-- Garden
GreenSpace <|-- MediumSizedPark
GreenSpace <|-- LargeSizedPark
Collaborator <|-- HRM
HRM "1" - "1..*" Skill : assigns >
Collaborator "1" -- "1..*" Skill : has >
GreenSpace "1" -- "1..*" Collaborator: contains >

@enduml
