@startuml
skinparam monochrome true
skinparam packageStyle rectangle
skinparam shadowing false

'left to right direction

skinparam classAttributeIconSize 0

hide circle
hide methods

'class Platform {
'  -description
'}

class Collaborator {
   name
   birthdate
   admissionDate
   address
   contactInfo
   idDocumentType
   idDocumentNumber
}

class Skill {
    name
}

class HRM {
   name
   birthdate
   admissionDate
   address
   contactInfo
   idDocumentType
   idDocumentNumber
}


Collaborator <|-- HRM
HRM "1" -- "1..*" Skill : assigns >
Collaborator "1" -- "1..*" Skill : has >

@enduml
