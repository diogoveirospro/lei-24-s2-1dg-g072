@startuml
skinparam monochrome true
skinparam packageStyle rectangle
skinparam shadowing false

participant "organization\n:Organization" as ORG
participant "skill\n:Skill" as SKILL

autonumber 7
-> ORG: registerJob(name)

autonumber 7.1
activate ORG
    group Register Skills

        ORG -> SKILL**: registers(name)
        ORG -> ORG: addSkill(skill)
        activate ORG
                ORG -> ORG: validateSkill(skill)
                activate ORG

                        ORG -> ORG
                deactivate ORG

                ORG -> ORG
        deactivate ORG
    end

    autonumber 8
    <-- ORG : job
deactivate ORG

@enduml