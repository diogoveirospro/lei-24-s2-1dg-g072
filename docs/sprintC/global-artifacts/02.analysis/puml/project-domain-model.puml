@startuml
skinparam monochrome true
skinparam packageStyle rectangle
skinparam shadowing false
'skinparam linetype ortho
'left to right direction
skinparam classAttributeIconSize 0
hide circle
hide methods


class HRM {

}

class Skill {
    name
}
class Job {
    name
}


class Team {
    size
    members

}

class Collaborator {
    name
    birthdate
    admissionDate
    address
    mobile number
    email
    password
    idDocumentType
    idDocumentNumber
    taxPayerNumber
}



class Vehicle{
    plate id
    brand
    model
    type
    tare
    gross weight
    current kms
    registration date
    acquisition date
    maintenance frequency
}

class VFM {

}

class Maintenance {
    plate number
    date of last maintenance
    km at maintenance
}

class GSM{

}
class "Collaborator" as Col {

}
class GreenSpace{
    name
    area
    dimension
    type
    greenSpaceManager
}
class Task{
    vehicleList
    team
    status
    duration
    task
}
class ToDoList{
    toDoListEntries
}
class AgendaEntry{
    startDate
    endDate
    status
    toDoListEntries
}
class Entry {
    startDate
    endDate
    status
    task
    vehicleList
    team
    greenSpace
    degree of urgency
}
class Agenda{
    entriesAgenda
}




'US001
HRM "1" -- "1..*" Skill: register >

'US002
HRM "1" -- "1..*" Job : register >

'US003
HRM "1" -- "1..*" Collaborator : registers >

'US004
HRM "1" -- "1..*" Skill : assigns >
Collaborator "1" -- "1..*" Skill : has >

'US005
HRM "1" -- "1..*" Team : creates >
Collaborator "1" -- "1" Job: has >
Team "1" -- "1..*" Collaborator: has >


'US006
VFM "1" -- "1..*" Vehicle: registers >

'US007
VFM "1"-- "1..*" Vehicle: chooses >

'US008
VFM "1" -- "1..*" Maintenance: lists >
Vehicle "1..*" -- "1" Maintenance: has <

'US020
"GSM" "1" -- "*" "Green Space" : register >

'US021
GSM "1" -- "0..*" GreenSpace : manages >
GreenSpace "1" -- "0..*" ToDoListEntry : associated with >
ToDoListEntry "1" -- "1" Task: has >
ToDoList "1" -- "0..*" ToDoListEntry : includes >
'US022
Agenda "1" -- "0..*" AgendaEntry : includes >
GSM "1" -- "1..*" AgendaEntry : add >

'US023
GSM "1" -- "1..*" Team : assign >
Team "1" -- "0..*" AgendaEntry : has >

'US024
AgendaEntry --|> Entry
GSM "1" -- "0..*" AgendaEntry : postpone >

'US025
GSM "1" -- "0..*" AgendaEntry : cancel >

'US026
GSM "1" -- "1..*" Vehicle : assign >
AgendaEntry "1" -- "0..*" Vehicle : has >

'US027
GSM "1" -- "1..*" GreenSpace: lists >

'US028
Col "1" -- "1..*" Task: lists >

'US029
Col "1"-- "   1..*" Task: complete >

@enduml